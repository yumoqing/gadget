{
	"logger":{
		"name":"gadget",
		"levelname":"debug",
		"logfile":"$[workdir]$/logs/gadget.log"
	},
	"filesroot":"$[workdir]$/files",
	"website":{
		"paths":[
			["$[workdir]$/wwwroot",""]
		],
		"host":"0.0.0.0",
		"port":9080,
		"coding":"utf-8",
		"ssl_gg":{
			"crtfile":"$[workdir]$/conf/www.bsppo.com.pem",
			"keyfile":"$[workdir]$/conf/www.bsppo.com.key"
		},
		"indexes":[
			"index.html",
			"index.tmpl",
			"index.dspy",
			"index.md"
		],
		"processors":[
			[".proxy","proxy"],
			[".xlsxds","xlsxds"],
			[".sqlds","sqlds"],
			[".tmpl.js","tmpl"],
			[".tmpl.css","tmpl"],
			[".html.tmpl","tmpl"],
			[".tmpl","tmpl"],
			[".ui","tmpl"],
			[".dspy","dspy"],
			[".md","md"]
		],
		"rsakey":{
			"privatekey":"$[workdir]$/conf/rsa_private_key.pem",
			"publickey":"$[workdir]$/conf/rsa_public_key.pem"
		}
	},
	"langMapping":{
		"zh-Hans-CN":"zh-cn",
		"zh-CN":"zh-cn",
		"en-us":"en",
		"en-US":"en"
	}
}
